<action>
	<icon>document-edit-sign</icon>
	<name>Create MD5 Report</name>
	<submenu></submenu>
	<unique-id>1751122085405998-2</unique-id>
	<command>xfce4-terminal --hold -e &apos;bash -c &quot;md5sum * &gt; checksums.md5; echo; echo Creates a checksums.md5 report for all files in this directory. Report saved to checksums.md5; echo; echo Close the terminal. Usually, SHIFT+CTRL+q; read&quot;&apos;</command>
	<description>Creates a checksums.md5 report for all files in this directory.</description>
	<range>*</range>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>org.xfce.terminal</icon>
	<name>Calculate SHA256 Checksum</name>
	<submenu></submenu>
	<unique-id>1751725518420178-1</unique-id>
	<command>xfce4-terminal --hold -e &quot;sha256sum %f&quot;</command>
