<action>
	<icon>org.xfce.terminalemulator</icon>
	<name>Calculate MD5 Checksum</name>
	<submenu></submenu>
	<unique-id>1751121111129604-1</unique-id>
	<command>xfce4-terminal --hold -e &quot;md5sum %f&quot;</command>
	<description>Calculate MD5 Checksum</description>
	<range>*</range>
	<patterns>*</patterns>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
