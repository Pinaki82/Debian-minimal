<action>
	<icon>document-edit-sign</icon>
	<name>Create SHA256 Report</name>
	<submenu></submenu>
	<unique-id>1751725780503400-2</unique-id>
	<command>xfce4-terminal --hold -e &apos;bash -c &quot;sha256sum * &gt; checksums.sha256; echo; echo Creates a checksums.sha256 report for all files in this directory. Report saved to checksums.sha256; echo; echo Press ENTER to close the terminal; read&quot;&apos;</command>
	<description>Creates a checksums.sha256 report for all files in this directory.</description>
	<range>*</range>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>document-edit-sign</icon>
	<name>Check md5 checksums</name>
	<submenu></submenu>
	<unique-id>1751905382198991-1</unique-id>
	<command>xfce4-terminal --hold -e &quot;md5sum --check %f&quot;</command>
	<description>Examine a file that includes md5 checksums paired with their respective filenames and validate the report. Right-click on such a file and select this option.</description>
	<range>*</range>
	<patterns>*</patterns>
